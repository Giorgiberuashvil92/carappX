{
  "name": "carappx",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "prebuild": "expo prebuild --clean",
    "start": "expo start",
  "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "dev:all": "./start-dev.sh",
    "clean-mongodb": "node scripts/clean-mongodb.js",
    "export-tokens": "node scripts/export-device-tokens.js",
    "build:ios": "eas build --platform ios --profile production",
    "build:android": "eas build --platform android --profile production",
    "build:all": "eas build --platform all --profile production",
    "build:ios:preview": "eas build --platform ios --profile preview",
    "build:android:preview": "eas build --platform android --profile preview",
    "build:ios:dev": "eas build --platform ios --profile development",
    "build:android:dev": "eas build --platform android --profile development",
    "build:android:local": "cd android && ./gradlew assembleRelease && echo '✅ APK built successfully at: android/app/build/outputs/apk/release/app-release.apk'",
    "build:android:local:debug": "cd android && ./gradlew assembleDebug && echo '✅ Debug APK built successfully at: android/app/build/outputs/apk/debug/app-debug.apk'",
    "build:android:local:aab": "cd android && ./gradlew bundleRelease && echo '✅ AAB built successfully at: android/app/build/outputs/bundle/release/app-release.aab'"
  },
  "engines": {
    "node": ">=20.19.4"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
